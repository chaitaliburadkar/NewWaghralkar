
@{
    ViewBag.Title = "BlogDetailIndex";
    Layout = "~/Views/Shared/AdminLayout .cshtml";
}
<script src="~/Scripts/jquery-3.7.1.min.js"></script>
<script src="~/Content/Blog.js"></script>

<h2>Blog Detail Index</h2>

<div class="container">
    <div class="col-sm-12">
        <div class="row">
            <div class="col-sm-6">
                <lable><b>ActiveId-</b></lable>
                <input value="@ViewBag.ActivityId" id="txtActiveId" />
            </div>

            <div class="col-sm-6">
                <lable><b>Title-</b></lable>
                <span id="txtTitle"></span>
            </div>

            <div class="col-sm-6">
                <lable><b>Details-</b></lable>
                <span id="txtDetails"></span>
            </div>

            <div class="col-sm-6">
                <lable><b>Image1-</b></lable>
                <span id="txtImage1"></span>
            </div>

            <div class="col-sm-6">
                <lable><b>Image2-</b></lable>
                <span id="txtImage2"></span>
            </div>

            <div class="col-sm-6">
                <lable><b>Type-</b></lable>
                <span id="txtType"></span>
            </div>

            <div class="col-sm-6">
                <lable><b>Date-</b></lable>
                <span id="txtDate"></span>
            </div>

        </div>

    </div>

</div>