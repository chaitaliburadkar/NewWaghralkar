    <!-- book an appointment -->
    <div class="appointment">
        <div class="container">
            <div class="col-md-9 appointment-left">
                <h3>FREE APPOINTMENT BOOKING</h3>
                <p>Book your Online Appointment for Consulting.</p>
            </div>
            <div class="col-md-3 appointment-right">
                <a href="#" class="btn btn-default btn-default_2 pull-left" data-toggle="modal" data-target="#applyModal_1">book your appointment</a>
                <div class="modal fade" id="applyModal_1" tabindex="-1" role="dialog" aria-labelledby="applyModalLabel" aria-hidden="true">
                    <div class="modal-dialog dialog_3">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                                <h4 class="modal-title" id="myModalLabel">
                                    <div class="head_4">
                                        <h3>Make An appointment Now</h3>
                                        <p>
                                            Id : <asp:Label ID="lblid" Text=""></asp:Label>
                                        </p>
                                    </div>
                                </h4>
                            </div>
                            <div class="modal-body">
                                <div name="row" class="register">
                                    <asp:TextBox name="name" id="txtname" placeholder="Name" required=""></asp:TextBox>
                                    <asp:TextBox name="email id" id="txtemailid" placeholder="Email id" required=""></asp:TextBox>
                                    <asp:TextBox name="mobile number" id="txtmob" placeholder="Mobile Number" required=""></asp:TextBox>
                                    <asp:TextBox id="txtcity" placeholder="City" required=""></asp:TextBox>
                                    <asp:RadioButtonList ID="RadioButtonList1" class="form-control "
                                                         RepeatDirection="Horizontal" RepeatLayout="Flow" Width="200px">
                                        <asp:ListItem>Male</asp:ListItem>
                                        <asp:ListItem>Female</asp:ListItem>
                                    </asp:RadioButtonList>
                                    <asp:TextBox class="date" id="datepicker" value="Appointment date" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Appointment date';}" required=""></asp:TextBox>
                                    <asp:TextBox type="text" id="txtmsg" required=""
                                                 TextMode="MultiLine"></asp:TextBox>
                                    <asp:Button ID="btnsubmit" Text="Submit" class="btn-success"
                                                onclick="btnsubmit_Click" />
                                </div>
                            </div>
                            <!---start-date-piker---->
                            <link rel="stylesheet" href="~/Content/css/jquery-ui.css" />
                            <script src="~/Content/js/jquery-ui.js"></script>
                            <script>
                                $(function () {
                                    $("#datepicker").datepicker();
                                });
                            </script>
                            <!---/End-date-piker---->

                        </div>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
